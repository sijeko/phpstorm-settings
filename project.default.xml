        <phpmd_settings>
          <MessDetectorConfiguration tool_path="$USER_HOME$/project/codestyle/php-mess-detector/vendor/bin/phpmd" />
        </phpmd_settings>
      </component>
      <component name="NodePackageJsonFileManager">
