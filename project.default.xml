        <option name="migrateCommands">
          <list>
            <MigrateCommand>
              <option name="isDefault" value="true" />
              <option name="command" value="migrate" />
              <option name="migrationTable" value="{{%migration}}" />
              <option name="db" value="db" />
              <option name="migrationPath">
                <list>
                  <option value="@app/migrations" />
                </list>
              </option>
            </MigrateCommand>
          </list>
        </option>
        <option name="aliasMap">
          <map>
            <entry key="@webroot" value="@app/web" />
            <entry key="@vendor" value="vendor" />
            <entry key="@runtime" value="@app/runtime" />
            <entry key="@yii2support-console-command-app-root" value="" />
          </map>
        </option>
