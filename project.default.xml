<application>
  <component name="ProjectManager">
    <defaultProject>
      <component name="BladeInjectionConfiguration" escapedTextEnd="!!}" escapedTextStart="{!!" />
      <component name="Encoding" addBOMForNewFiles="with NO BOM">
        <file url="PROJECT" charset="UTF-8" />
      </component>
      <component name="EslintConfiguration">
        <custom-configuration-file used="true" path="$USER_HOME$/project/codestyle/eslint/eslintrc.js" />
      </component>
      <component name="InspectionProjectProfileManager">
        <profile version="1.0">
          <option name="myName" value="Project Default" />
          <inspection_tool class="Eslint" enabled="true" level="ERROR" enabled_by_default="true" />
          <inspection_tool class="PhpCSValidationInspection" enabled="true" level="WEAK WARNING" enabled_by_default="true">
            <option name="CODING_STANDARD" value="Custom" />
            <option name="CUSTOM_RULESET_PATH" value="$USER_HOME$/project/codestyle/php-code-sniffer/Sijeko" />
          </inspection_tool>
        </profile>
        <version value="1.0" />
      </component>
      <component name="JSHintConfiguration" version="2.9.4" use-config-file="true" use-custom-config-file="true" custom-config-file-path="$USER_HOME$/project/codestyle/jshint/jshint-options.js">
